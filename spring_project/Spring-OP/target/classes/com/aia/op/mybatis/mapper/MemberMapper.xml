<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- sql 설정: sql문, 반환타입, 파마리터 설정 -->
<mapper namespace="com.aia.op.mybatis.mapper.MemberMapper">

<insert id="insertMember" parameterType="com.aia.op.member.model.Member">

INSERT INTO project.member (uid, upw, uname, uphoto ) VALUES (#{uid},#{upw},#{uname},#{uphot})

</insert>






</mapper>	
	
